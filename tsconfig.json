{
  "compilerOptions": {
    "lib": [
         "es5",
         "es6"
      ],
    "target": "es5",                          
    "module": "commonjs", 
    "moduleResolution": "node",                
    "allowJs": true,
    "outDir": "./dist",
    "strict": true,         
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true
  }
}